{
  "name": "@juancwu/crwl",
  "version": "0.0.1",
  "description": "Extensible web crawler with page object design pattern.",
  "main": "build/cjs/crwl/index.js",
  "type": "commonjs",
  "module": "build/esm/crwl/index.js",
  "types": "build/types.d.ts",
  "exports": {
    ".": {
      "types": "./build/types.d.ts",
      "import": "./build/esm/crwl/index.js",
      "require": "./build/cjs/crwl/index.js"
    },
    "./*": {
      "import": "./*",
      "require": "./*"
    }
  },
  "scripts": {
    "license": "gen license",
    "generate:types": "npm run clean:docs && api-extractor run --local --verbose",
    "clean": "npm run clean:build && npm run clean:doc",
    "clean:build": "rimraf build",
    "clean:docs": "rimraf docs-api-json",
    "build": "npm run clean:build && npm run build:tsc && npm run generate:types",
    "build:tsc": "tsc --version && (npm run build:tsc:cjs && npm run build:tsc:esm)",
    "build:tsc:cjs": "tsc -b tsconfig.csj.json",
    "build:tsc:esm": "tsc -b tsconfig.esm.json"
  },
  "keywords": [
    "web-scraping",
    "web",
    "crawling",
    "automation"
  ],
  "author": "Juan Wu",
  "license": "MIT",
  "dependencies": {
    "cheerio": "^1.0.0-rc.11",
    "mkdirp": "^1.0.4",
    "nanoid": "^3.3.4",
    "puppeteer": "^14.4.1"
  },
  "devDependencies": {
    "@microsoft/api-documenter": "^7.17.19",
    "@microsoft/api-extractor": "^7.25.2",
    "@types/mkdirp": "^1.0.2",
    "generate": "^0.14.0",
    "generate-license": "^1.0.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4"
  },
  "files": [
    "build"
  ],
  "private": false,
  "homepage": "https://github.com/juancwu/crwl",
  "bugs": {
    "url": "https://github.com/juancwu/crwl/issues"
  }
}
